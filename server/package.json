{
  "name": "website-boilerplate-server",
  "version": "1.0.0",
  "description": "Server portion of Website Boilerplate",
  "main": "./runtime/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "run-s clean:dev compile:dev copy-files:dev copy-images:dev",
    "build:prod": "run-s clean:prod compile:prod copy-files:prod copy-images:prod",
    "clean:dev": "rimraf runtime",
    "clean:prod": "rimraf runtime",
    "compile:dev": "tsc",
    "compile:prod": "tsc -p \"./tsconfig.prod.json\"",
    "copy-files:dev": "shx cp \"./public/{robots.txt,favicon.png}\" \"./dist/\"",
    "copy-files:prod": "shx cp \"./public/{robots.txt,favicon.png}\" \"./dist/\"",
    "copy-images:dev": "shx cp -r \"./public/images/static/*\" \"./dist/i/s/\"",
    "copy-images:prod": "shx cp -r \"./public/images/static/*\" \"./dist/i/s/\"",
    "start:dev": "cross-env NODE_ENV=development node runtime/app.js",
    "start:prod": "cross-env NODE_ENV=production node runtime/app.js"
  },
  "author": "Lieutenant Crunch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LieutenantCrunch/website-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/LieutenantCrunch/website-boilerplate/issues"
  },
  "homepage": "https://github.com/LieutenantCrunch/website-boilerplate#readme",
  "dependencies": {
    "args": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "file-type": "^16.5.0",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "memoizee": "^0.4.15",
    "mime-types": "^2.1.30",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.6.1",
    "sequelize": "^6.6.2",
    "sequelize-simple-cache": "^1.3.0",
    "sharp": "^0.27.2",
    "socket.io": "^4.1.2",
    "uuid": "^8.3.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/args": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/fluent-ffmpeg": "^2.1.17",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/memoizee": "^0.4.5",
    "@types/mime-types": "^2.1.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.17.1",
    "@types/nodemailer": "^6.4.2",
    "@types/sequelize": "^4.28.9",
    "@types/sharp": "^0.27.3",
    "@types/uuid": "^8.3.0",
    "@types/zxcvbn": "^4.4.1",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shx": "^0.3.3",
    "typescript": "^4.3.4"
  }
}
